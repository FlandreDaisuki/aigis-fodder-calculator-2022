{
  "eslint.experimental.useFlatConfig": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": [
    "Aigis",
    "demi",
    "pinia",
    "unocss",
    "vueuse"
  ],
  "cSpell.ignoreWords": [
    "antfu",
    "intlify",
    "unplugin"
  ],
  "i18n-ally.sourceLanguage": "ja",
  "i18n-ally.displayLanguage": "zh-TW",
  "i18n-ally.localesPaths": [
    "locales"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ]
}
